<%= simple_form_for @article do |f| %>
  <%= f.input :title, label: 'Titre' %>
  <%= f.input :description,  :maxlength => 240 %>
  <%= f.input :url, label: "Lien de l'article" %>
  <%= f.association :tags,   as: :check_boxes, collection_select: :selected %>
  <%= f.submit "Enregister", class: "btn btn-primary" %>
<% end %>
